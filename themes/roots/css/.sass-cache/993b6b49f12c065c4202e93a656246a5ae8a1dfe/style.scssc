/* Global & Overall
------------------------------------------------------------------------------------*/



body {}

p {
	line-height: 1.7;
	color: #333;
}

.text-box, #sidebar, figure {
	margin: 1em 0;
}

/* Banner
------------------------------------------------------------------------------------*/

#banner {
	background: #404040 url(../img/banner.jpg) no-repeat center top;
}

#logo {
	display: block;
	text-align: center;
	padding: 1em 0;
}

#brand, #phone {
	font-size: 1.5em;
	text-align: center;
	font-weight: bold;
	text-shadow: -1px 1px 0px $light-gray, -3px 2px darken($dark-gray, 20%);
}

#brand a {
	color: #fff;
	font-style: italic;
	font-weight: bold;
	&:hover {
		text-decoration: none;
	}
}

#phone {
}

/* Headings
------------------------------------------------------------------------------------*/

h1, h3 {
	font-style: italic;
	text-transform: uppercase;
	letter-spacing: 0.1em;
}
h1, h3, #phone, #sidebar {
	color: #fff;
}

h3 {
	font-weight: normal;
}

#main h2 {
	color: $sidebar-blue;
}

.page-title {
	color: #000;
	font-size: 2em;
	margin: 0.4em 0;
}

/* Navigation
------------------------------------------------------------------------------------*/

a {
	color: $sidebar-blue;
	text-decoration: none;
	-webkit-transition: all 0.2s ease;
	-moz-transition: all 0.2s ease;
	transition: all 0.2s ease;
	&:hover {
		text-decoration: underline;
		color: #000;
	}
}

#sidebar {
	a {
		color: #333;
	}
	h3 {
		a {
			color: #fff;
			text-decoration: none;
			&:hover {
				font-style: italic;
			}
		}
	}
}

h4 a {
	font-weight: bold;
}

#nav-main {
}

#nav-main ul {
	list-style: none;
	margin-bottom: 0;
}

#nav-main li {

}

#nav-main a {
	color: #fff;
	display: block;
	height: 35px;
	line-height: 40px; /* Center menu items vertically */
	background: url(../img/menu.png) repeat-x;
	padding: 0 1em;
	text-shadow: 0px -1px 0px #555;
	letter-spacing: 0.1em;
	border-top: 1px solid #444;
	font-weight: bold;
	letter-spacing: 0.15em;
	-webkit-transition: all 0.2s ease;
	-moz-transition: all 0.2s ease;
	transition: all 0.2s ease;
}

#nav-main a:hover,
#nav-main .active a,
.single-products #nav-main .menu-products a {
	text-decoration: none;
	-webkit-text-shadow: 0px 0px 5px #000;
	-moz-text-shadow: 0px 0px 5px #000;
	-ms-text-shadow: 0px 0px 5px #000;
	-o-text-shadow: 0px 0px 5px #000;
	text-shadow: 0px 0px 5px #000;
	cursor: default;
	background-position: 0 -35px;
}

#nav-main a:hover {
	cursor: pointer;
}

/* Content
------------------------------------------------------------------------------------*/

#wrap {
	margin: 0 auto;
}

#sidebar, .text-box {
	padding: 1em 5%;
}

.text-box, #content-info {
	background: $content-bg;
}

.gallery {
	list-style: none;
}

.gallery-item {
	margin: 0 2% 2% 0;
	img {
		border: 1px solid #000;
	}
}

#sidebar {
	img {
		border: 1px solid #000;
	}
}

#main {
	margin: 1em;
}

.home #main {
	margin: 0;
}

.phone {
	font-weight: bold;
	font-size: 1.5em;
}

/* Figures
------------------------------------------------------------------------------------*/

/* Webkit reset */
figure {
	-webkit-margin-before: 0;
	-webkit-margin-after: 0;
	-webkit-margin-start: 0;
	-webkit-margin-end: 0;
}

.afc-product {
	width: 90%;
	margin: 0 auto;
	figure {
		margin: 1em auto;
		img {
			border: 1px solid #000;
		}
	}
}

figure {
	text-align: center;
	img {
		display: inline-block;
	}
}

/* Sidebar
------------------------------------------------------------------------------------*/

#sidebar {
	background: #0091bd;
	h3 {
		border-bottom: 1px solid #fff;
		width: 100%;
	}
	h3, p {
		color: #fff;
	}
	
}

#sidebar-products {
	list-style: none;
	padding: 0;
	margin: 0.5% auto;
	figure {
		margin: 1em 0 0;
	}
	a {
		width: 41%;
		float: left;
		margin: 0 4% 3% 0;
		padding: 2.5%;
		text-align: center;
		color: #fff;
		&:hover {
			background: lighten($sidebar-blue, 10%);
			color: #fff;
			text-decoration: none;
		}
	}
}

.featured-image {
	margin: 0 auto;
}

/* Footer
------------------------------------------------------------------------------------*/

#content-info {
	padding: 1em;
	p {
		margin: 0;
		font-size: 0.9em;
		color: lighten(#444, 20%);
	}
	text-align: center;
}

#certs {
	display: inline-block;
	img {
		opacity: 0.6;
		-webkit-transition: opacity 0.2s linear;
		-moz-transition: opacity 0.2s linear;
		transition: opacity 0.2s linear;
		&:hover {
			opacity: 1;
		}
	}
}

@media only screen and (min-width: 480px){}

@media only screen and (min-width: 600px){

/* Banner
------------------------------------------------------------------------------------*/

	#banner {
		background: #404040 url(../img/banner.jpg) no-repeat -150px top;
	}
	
	#logo {
		display: block;
		float: left;
		padding: 0;
		margin: 1em;
	}
	
	#brand {
		font-size: 2em;
	}
	
	#brand, #phone {
		position: relative;
		top: 1em;
	}

/* Navigation
------------------------------------------------------------------------------------*/

	#nav-main {
		clear: both;
		ul {
		
		}
		li {
			&:first-child a {
				border-left: 0;
			}
		}
		a {
			padding: 0;
			width: 24.85%;
			float: left;
			text-align: center;
			font-size: 0.9em;
			border-top:0;
			border-left: 1px solid #444;
		}
	}

/* Content & Sidebar
------------------------------------------------------------------------------------*/

	.afc-product {
		width: 43%;
		float: left;
		margin-left: 5%;
		height: 32em;
		p {
			margin-bottom: 0;
		}
	}
	
	.gallery-item {
		float: left;
	}
	
	#main p, .page-title {
		clear: left;
	}

	#sidebar, #primary-content, #secondary-content {
		margin: 1em 0;
		padding: 2%;
		height: auto;
	}
	
	.home #sidebar, #primary-content, #secondary-content {
		width: 45%;
	}
	
	.home #sidebar {
		padding: 2%;
	}
	
	#sidebar {
		padding: 1.5%;
		width: 100%;
	}
	
	
	#sidebar-products a {
		width: 22%;
		margin: 2%;
	}
	
	#sidebar, #featured-image {
		float: left;
		clear: left;
	}
	
	#featured-image {
		padding: 0;
		width: 49%;
		margin: 0;
	}
	
	#primary-content, #secondary-content {
		float: right;
		clear: right;
	}
	
	#secondary-content {
		margin-top: 0;
	}

	#content-info {
		clear: both;
	}

}

@media only screen and (min-width: 768px){


}

@media only screen and (min-width: 992px){

	body {
		max-width: 1200px;
	}

/* Banner
------------------------------------------------------------------------------------*/

	#banner {
		background: #404040 url(../img/banner.jpg) no-repeat -150px top;
		position: relative;
	}
	
	#brand {
		text-align: left;
		float: left;
	}
	
	#brand {
		top: 1.8em;
	}
	
	#phone {
padding: 0;
right: 1em;
position: absolute;
top: 2.5em;
	}
	
/* Nav
------------------------------------------------------------------------------------*/

	#nav-main {
		width: 80%;
		clear: none;
		position: absolute;
		right: 0;
		bottom: 0;
		li:first-child a {
			border-radius: 1em 0 0 0;
		}
	}

/* Content & Sidebar
------------------------------------------------------------------------------------*/
	
	#sidebar {
		width: 21%;
		float: left;
	}
	
	.home #sidebar {
		width: 20.5%;
	}
	
	#sidebar-products {
		a {
			margin: 0 4% 3% 0;
			width: 41%;
		}
	}
	
	#main {
		width: 74%;
		margin: 0;
		p {
			margin: 0 1em 1em 0;
		}
	}
	
	#primary-content {
		float: left;
	}
	
	#featured-image {
		margin-top: 1em;
		clear: none;
	}
	
	#featured-image, #secondary-content, #main {
		float: right;
	}
	
	.post-type-archive-products #main {
		width: 100%;
	}
	
	.afc-product {
		width: 22%;
		float: left;
		margin-left: 2.5%;
		height: 34em;
		p {
			margin-bottom: 0;
			font-size: 0.9em;
		}
	}
/* Footer
------------------------------------------------------------------------------------*/

	#certs {
		display: block;
		float: right;
	}
}

